    ],
    "http://schema.org/image": [
      {
        "@id": "https://evergreenculturalcentre.ca/wp-content/uploads/2025/10/paulina-herpel-aeOvJ24HnFE-unsplash-scaled.jpg"
      }
    ],
    "http://schema.org/location": [
