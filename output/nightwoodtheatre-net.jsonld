  {
    "@id": "https://www.nightwoodtheatre.net/temporary/b3f4683b-497e-437a-95dc-e67595969d7e",
    "@type": [
      "https://schema.org/WebPage"
    ],
    "http://www.w3.org/ns/prov#wasDerivedFrom": [
