    "http://schema.org/headline": [
      {
        "@language": "fr",
        "@value": "Dear Criminals sera à Rouyn pour présenter LONE RIDE un micro spectacle privé d’une chanson, une présentation de Newmont Éléonore"
      }
    ],
    "http://schema.org/wordCount": [
      {
        "@language": "fr",
        "@value": "403"
      }
    ],
